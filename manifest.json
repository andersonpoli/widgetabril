{
	"name": "Abril WidgetChrome",
	"version": "0.0.1",
	"manifest_version": 2,
	"permissions": [
        "*://busca.abril.com.br/public/search/v2/*"
    ],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://www.iba.com.br; object-src 'self'",
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_title": "Abril App Chrome",
		"default_popup": "index.html"
	}
}